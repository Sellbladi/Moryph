local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/RegularVynixu/IreXion-UI-Library/main/IreXion%20UI%20Library"))()

local Gui = Library:AddGui({
	Title = {"Moryph!", "Da Hood Modded!"},
	ThemeColor = Color3.fromRGB(125, 30, 250),
	ToggleKey = Enum.KeyCode.RightShift,
})

local Tab = Gui:AddTab("Player")

local Category = Tab:AddCategory("Walkspeed")

local Button = Category:AddButton("Walkspeed (C)", function()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/angeloX041/Speedc/main/SuperSpeedC ", true))()
end)

local Category = Tab:AddCategory("Avatar")

local Button = Category:AddButton("Right Korblox", function()
	loadstring(game:HttpGet('https://raw.githubusercontent.com/carlcoded/pro/main/rightkorbloxperma'))()
end)

local Button = Category:AddButton("Left Korblox", function()
	loadstring(game:HttpGet('https://raw.githubusercontent.com/carlxdd/misc-releases/main/leftkorblox'))()
end)

local Button = Category:AddButton("Headless", function()
	loadstring(game:HttpGet('https://raw.githubusercontent.com/carlcoded/pro/main/headlessperma'))()

end)

local Button = Category:AddButton("Face Changer", function()
	loadstring(game:HttpGet('https://raw.githubusercontent.com/carlcoded/Scripts/main/carlface'))()

end)


local Category = Tab:AddCategory("Animations")

